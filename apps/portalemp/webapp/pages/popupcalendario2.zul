	   <textbox  tabindex="1" id="NOMBRE" placeholder="@load(vm.lang.getProperty('NOMBRE.label', 'nombre'))" value="@bind(vm.beanData.nombre) @save(vm.beanData.nombre, before=('store')) @validator(vm.validator,maxl='100' ,oldValue=vm.beanDataCpy.nombre)"  sclass="form-control  form-control border" visible="@load(vm.isVisible('NOMBRE'))" disabled="@load(vm.isEditable('NOMBRE')?false:true)"/>
       <label sclass="text-danger" value="@load(vmsgs['NOMBRE'])"/>
    </h:div>   








   
