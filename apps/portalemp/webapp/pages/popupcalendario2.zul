<h:div class="col-md-12">
<label sclass="form-label font-weight-bold text-end" value="Días Festivos y especiales" style=" Font-Size:16px;"/>
</h:div>



 
 <div class="col-lg-12">		
  <grid id="subrrhhfestivos" height="400px" width="100%" 
   model="@bind(vm.grid['subrrhhfestivos']) " 
    
    onCreate="@command('onSubtableCreate',component=self,alias='subrrhhfestivos')">
    <auxhead  sclass="bg-soft-light" >
           <auxheader  colspan="1" rowspan="1" >
           <combobox mold="rounded" sclass=""  model="@load(vm.entitys['subrrhhfestivos'].getFieldByAtribute('TIPO').getFieldPropsBeanByID('txValues').enums)" 
selectedItem="@bind(vm.beans['subrrhhfestivos'].tipo)"  value="@bind(vm.beans['subrrhhfestivos'].tipo)  ">
<template name="model" var="item">
<comboitem value="${item.stValue ne null ? item.stValue:item.stValue}" label="${item.stValue ne null?item.stValue:item.inValue}"/>
</template>
</combobox>
           </auxheader>
           <auxheader  colspan="1" rowspan="1" >
           <datebox value="@bind(vm.beans['subrrhhfestivos'].fecha)"/>
           </auxheader>
<auxheader  colspan="1" rowspan="1" >
<textbox value="@bind(vm.beans['subrrhhfestivos'].descripcin)"/>
</auxheader>
<auxheader  colspan="1" rowspan="1" >
<button label="añadir" onClick="@command('appendRowForm',alias='subrrhhfestivos')" />
</auxheader>
        </auxhead>
	  <columns sizable="true">
   
      
	   
        <column label="@load(vm.lang.getProperty('TIPO.label', 'TIPO'))" align="center" sclass="text-capitalize"/>
   
      
	   
        <column label="@load(vm.lang.getProperty('FECHA.label', 'FECHA'))" align="center" sclass="text-capitalize"/>
   
      
	   
        <column label="@load(vm.lang.getProperty('DESCRIPCIN.label', 'DESCRIPCIN'))" align="center" sclass="text-capitalize"/>
   
      
        <column align="center" />
	 </columns>
      <template name="model" var="item">
      	<row style="text-align:right;">
    	  				     <label  value="@load(item.tipo) @converter(vm.fieldConverter)"></label>
				     <label  value="@load(item.fecha) @converter(vm.fieldConverter)"></label>
				     <label  value="@load(item.descripcin) @converter(vm.fieldConverter)"></label>
               <cell align="center">
						<button
						onClick="@command('cancelRowForm',alias='subrrhhfestivos',item=item)"
						iconSclass="s-icon s-icon-trash"
						sclass="btn" />
		                </cell> 
       </row>
   	</template>
  </grid>
