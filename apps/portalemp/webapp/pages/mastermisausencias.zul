	   </hbox>
    <datebox     id="556403"  placeholder="@load(vm.lang.getProperty('FIN.label', 'fin'))" tabindex="2" value="@bind(vm.beanData.fin) @save(vm.beanData.fin, before=('grabar')) "   format="dd/MM/yyyy"  
