	    <a iconSclass="su-icon su-icon-question" popup="pop_INICIO" />
	   </hbox>
    <datebox     id="605831"  placeholder="@load(vm.lang.getProperty('INICIO.label', 'inicio'))" tabindex="1" value="@bind(vm.beanData.inicio) @save(vm.beanData.inicio, before=('grabar')) @validator(vm.validator,required=true ,oldValue=vm.beanDataCpy.inicio)"   format="dd/MM/yyyy"  
    visible="@load(vm.isVisible('INICIO'))" disabled="@load(vm.isEditable('INICIO')?false:true)" sclass="margin-field"/>
    <label sclass="text-danger" value="@load(vmsgs['605831'])"/>
