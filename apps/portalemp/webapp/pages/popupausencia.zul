	    <a iconSclass="su-icon su-icon-question" popup="pop_NUMHORAS" />
	   </hbox>
	      <intbox   tabindex="0" id="FzzmVtfcmw"  placeholder="@load(vm.lang.getProperty('NUMHORAS.label', 'Número Horas'))" sclass="margin-field form-control  form-control border "  value="@bind(vm.beanData.numhoras) @save(vm.beanData.numhoras, before=('grabar')) @validator(vm.validator,max=8 ,min=1 ,oldValue=vm.beanDataCpy.numhoras)" visible="@load(vm.isVisible('NUMHORAS'))" disabled="@load(vm.isEditable('NUMHORAS')?false:true)" onChange="@command('onChangeInput',field='NUMHORAS',evento=event,componente=self,calculate=true)" />
    <label sclass="text-danger" value="@load(vmsgs['FzzmVtfcmw'])"/>   
    
    </h:div>




         </div>
      </tabpanel>   
      <tabpanel>
        <div class="row">


				


    <h:div class="col-md-4 border-field">
       <hbox spacing="3px" visible="@load(vm.isVisible('INICIO'))" sclass="margin-field">
	    <label sclass="fs-6 text form-label  text-capitalize  font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('INICIO.label', 'Desde'))"/>
	    <a iconSclass="su-icon su-icon-question" popup="pop_INICIO" />
	   </hbox>
	      <datebox     id="GGpthJjzgC"  placeholder="@load(vm.lang.getProperty('INICIO.label', 'Desde'))" tabindex="0" value="@bind(vm.beanData.inicio) @save(vm.beanData.inicio, before=('grabar')) @validator(vm.validator,required=true ,oldValue=vm.beanDataCpy.inicio)"   format="dd/MM/yyyy"  
    visible="@load(vm.isVisible('INICIO'))" disabled="@load(vm.isEditable('INICIO')?false:true)" sclass="margin-field"/>
    <label sclass="text-danger" value="@load(vmsgs['GGpthJjzgC'])"/>   
    
      </h:div>






				


    <h:div class="col-md-4 border-field">
       <hbox spacing="3px" visible="@load(vm.isVisible('FIN'))" sclass="margin-field">
	    <label sclass="fs-6 text form-label  text-capitalize  font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('FIN.label', 'Hasta'))"/>
	    <a iconSclass="su-icon su-icon-question" popup="pop_FIN" />
	   </hbox>
	      <datebox     id="dFuPWKCZQV"  placeholder="@load(vm.lang.getProperty('FIN.label', 'Hasta'))" tabindex="0" value="@bind(vm.beanData.fin) @save(vm.beanData.fin, before=('grabar')) @validator(vm.validator,min=fx.INICIO ,oldValue=vm.beanDataCpy.fin)"   format="dd/MM/yy"  
    visible="@load(vm.isVisible('FIN'))" disabled="@load(vm.isEditable('FIN')?false:true)" sclass="margin-field"/>
    <label sclass="text-danger" value="@load(vmsgs['dFuPWKCZQV'])"/>   
    
      </h:div>




         </div>
      </tabpanel>   
     </tabpanels>
</tabbox> 



</div>
</div>                     </div>           
            </h:div>
            <div class="hstack gap-2 justify-content-end">
								
									<button onClick="@command('grabar',action='continuar')"
										class="btn btn-success" >
										Guardar
									</button>
									<button onClick="@command('grabar',action='nuevo')"
										class="btn " >
										Guardar y Nuevo
									</button>
									<button onClick="@command('cerrarPopup')"
										class="btn btn-light" >
										Cerrar
									</button>
								
							</div>
           </h:div>
          </h:div>
         </div>   
        		</panelchildren>
		</panel> 
            <fragment exps="@load(vm.lang['Ausencia.screen'])" >
				<![CDATA[
				<div id="modal_iconified" class="modal fade" tabindex="-1" style="z-index: 99999;">
					<div class="modal-dialog">
						<div class="modal-content">
							<div class="modal-header">
								<h5 class="modal-title"><span textContent='@load(exps)'></span></h5>
							</div>
							<div class="modal-body">
								<h6 class="font-weight-semibold"><i class="icon-law mr-2"></i>Texto de ayuda pantalla</h6>
								<span textContent='@load(exps)'></span>
							</div>
							<div class="modal-footer">
								<button class="btn btn-link" data-dismiss="modal"><i class="icon-cross2 font-size-base mr-1"></i> Close</button>
							</div>
						</div>
					</div>
				</div>
				]]>
				</fragment>
                 









                  
 </idspace>
