	    <a iconSclass="su-icon su-icon-question" popup="pop_FAPROVACION" />
	   </hbox>
	      <datebox  readonly="true"   buttonVisible="false"  id="KjxQPtEQAR"  placeholder="@load(vm.lang.getProperty('FAPROVACION.label', 'Fecha Aprobación'))" tabindex="3" value="@bind(vm.beanData.faprovacion) @save(vm.beanData.faprovacion, before=('grabar')) "   format="dd/MM/yyyy"  
    visible="@load(vm.isVisible('FAPROVACION'))" disabled="@load(vm.isEditable('FAPROVACION')?false:true)" sclass="margin-field"/>
    <label sclass="text-danger" value="@load(vmsgs['KjxQPtEQAR'])"/>   
    
      </h:div>





<tabbox orient="top" mold="default" >
     <tabs>
                 <tab label="Horas" />      
                 <tab label="Días" />      
     </tabs>
     <tabpanels>
      <tabpanel>
        <div class="row">


				


  <h:div class="col-md-6 border-field">
       <hbox spacing="3px" visible="@load(vm.isVisible('INICIOHORA'))" sclass="margin-field">
	    <label sclass="fs-6 text form-label  text-capitalize  font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('INICIOHORA.label', 'Hora/Día Inicio'))"/>
	    <a iconSclass="su-icon su-icon-question" popup="pop_INICIOHORA" />
	   </hbox>
	<datebox     id="ppzaUelwop" cols="20"  tabindex="0" value="@bind(vm.beanData.iniciohora) @save(vm.beanData.iniciohora, before=('grabar')) "   format="dd/MM/yy HH:mm" placeholder="@load(vm.lang.getProperty('INICIOHORA.label', 'Hora/Día Inicio'))" 
    visible="@load(vm.isVisible('INICIOHORA'))" disabled="@load(vm.isEditable('INICIOHORA')?false:true)" sclass="margin-field"/>
    <label sclass="text-danger" value="@load(vmsgs['ppzaUelwop'])"/>   
    
    </h:div>






				


    <h:div class="col-md-6 border-field">
