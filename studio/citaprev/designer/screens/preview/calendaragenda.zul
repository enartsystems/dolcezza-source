      <toolbar align="end">
      
          <toolbarbutton 
				iconSclass="mdi su-2x mdi-file-pdf" onClick="@command('report',type='pdf')" tooltiptext="View Pdf"/>
		<toolbarbutton 
				iconSclass="mdi su-2x mdi-file-excel" onClick="@command('report',type='excel')" tooltiptext="View Excel"/>
  	</toolbar>
		<div sclass="row">
			   <div id="divTemplate" sclass="col-lg-12" hflex="1"></div>

		</div>
