<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="altaempleado" name="Alta de Empleado" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="viewaltaempleado" name="Alta Empleado" activiti:candidateGroups="CLIENTE" activiti:formKey="portalemp|formaltaemploy.zul"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="viewaltaempleado"></sequenceFlow>
    <serviceTask id="mailtask1" name="Notificacion" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${var:get(ADMIN)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="cc">
          <activiti:expression><![CDATA[${var:get(ADMINCC)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="bcc">
          <activiti:expression><![CDATA[${var:get(ADMINBC)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[Nuevo empleado de ${var:get(CUSTOMER_NAME)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[Hola,
El cliente  ${var:get(CUSTOMER_NAME)} , ha notificadoel alta de un nuevo empleado
Por favor accede a tu bandeja de tarea para continuar con la tramitación.

Suinsit Platform]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow2" sourceRef="viewaltaempleado" targetRef="mailtask1"></sequenceFlow>
    <userTask id="usertask1" name="Revisar Alta" activiti:candidateGroups="COMERCIAL,OPERADOR,ADMINISTRACION" activiti:formKey="portalemp|formrevisaemploy.zul"></userTask>
    <sequenceFlow id="flow3" sourceRef="mailtask1" targetRef="usertask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <userTask id="usertask2" name="¿Son correctos los datos?" activiti:formKey="confirm|¿Son correctos los datos?"></userTask>
    <sequenceFlow id="flow5" sourceRef="usertask2" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="mailtask2" name="Confirmar" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${var:get(CUSTOMER_USER_EMAIL)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Alta empleado completada]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[Hola ${var:get(CUSTOMER_NAME)},
El alta de tu empleado a sido tramitada.]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="mailtask3" name="Notificar" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${var:get(CUSTOMER_USER_EMAIL)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[Hola ${var:get(CUSTOMER_NAME)} 
Necesitamos ampliar información para el alta de su nuevo empleado.
Por favor accede a tu bandeja de tarea para continuar con la tramitación.

Suinsit Platform]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Información Pendiente]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow6" name="Sí" sourceRef="exclusivegateway1" targetRef="mailtask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${var:get(continuar) ==true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" name="No" sourceRef="exclusivegateway1" targetRef="mailtask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${var:get(continuar) ==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="mailtask2" targetRef="endevent1"></sequenceFlow>
    <userTask id="usertask3" name="Revisar alta" activiti:candidateGroups="CLIENTE" activiti:formKey="portalemp|formrevisaemploycli.zul"></userTask>
    <serviceTask id="mailtask4" name="Notificar" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${var:get(ADMIN)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="cc">
          <activiti:expression><![CDATA[${var:get(ADMINCC)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="bcc">
          <activiti:expression><![CDATA[${var:get(ADMINBC)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[revisado alta empleado de ${var:get(CUSTOMER_NAME)}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[Hola,
El cliente  ${var:get(CUSTOMER_NAME)} , ha revisado el alta de un nuevo empleado
Por favor accede a tu bandeja de tarea para continuar con la tramitación.

Suinsit Platform]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow9" sourceRef="mailtask3" targetRef="usertask3"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="usertask3" targetRef="mailtask4"></sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="mailtask4" targetRef="usertask1"></sequenceFlow>
  </process>
