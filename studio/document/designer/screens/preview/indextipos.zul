	      <datebox     id="CkOpeBLCxs"  placeholder="@load(vm.lang.getProperty('BAJA.label', 'Fecha de baja'))" tabindex="3" value="@bind(vm.beanData.baja) @save(vm.beanData.baja, before=('grabar')) "   format="dd/MM/yyyy"  
