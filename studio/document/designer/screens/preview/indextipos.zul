 <checkbox mold="default" id="CZzGmMVnnj" value="@bind(vm.beanData.subcarpeta) @save(vm.beanData.subcarpeta, before=('grabar'))" sclass="margin-field form-check-input" visible="@load(vm.isVisible('SUBCARPETA'))" disabled="@load(vm.isEditable('SUBCARPETA')?false:true)" checked="@bind(vm.beanData.subcarpeta) @save(vm.beanData.subcarpeta, before=('grabar'))"/>
</h:div>





				


