<zk xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul/zul.xsd"
	xmlns:h="xhtml"
	xmlns:d="http://www.zkoss.org/2005/zk/client/attribute">
<div class="table">					 
					AAAAAAAAAAAAAAAAA
				 <grid  width="100%" height="500px"	model="@load(vm.beans)" sclass="table align-middle mb-0" >  
							<columns sizable="true" menupopup="auto">
							       <column hflex="1" label="@load(vm.lang.getProperty('NOMBRE.label', 'Nombre'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['NOMBREAsc'])" sortDescending="@load(vm.comparators['NOMBREDesc'])"/>
							       <column hflex="1" label="@load(vm.lang.getProperty('REFERENCIA.label', 'referencia'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['REFERENCIAAsc'])" sortDescending="@load(vm.comparators['REFERENCIADesc'])"/>
							       <column hflex="1" label="@load(vm.lang.getProperty('COMENTARIOS.label', 'comentarios'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['COMENTARIOSAsc'])" sortDescending="@load(vm.comparators['COMENTARIOSDesc'])"/>
							       <column hflex="1" label="@load(vm.lang.getProperty('EMAILCUSTOMER.label', ''))" align="center" sort="auto"  sortAscending="@load(vm.comparators['EMAILCUSTOMERAsc'])" sortDescending="@load(vm.comparators['EMAILCUSTOMERDesc'])"/>
 							      <column hflex="1" label="Acciones" align="center" />
							</columns >
							<template name="model" var="item">
                  			<row style="text-align:right;">
							         <label  value="@load(item.getFieldByAtribute('NOMBRE').value) @converter(vm.fieldConverter)"></label>
                                   
							         <label  value="@load(item.getFieldByAtribute('REFERENCIA').value) @converter(vm.fieldConverter)"></label>
                                   
							       <label  value="@load(item.getFieldByAtribute('COMENTARIOS').value) @converter(vm.converter, type='HTML',size=60)"></label>
							         <label  value="@load(item.getFieldByAtribute('EMAILCUSTOMER').value) @converter(vm.fieldConverter)"></label>
                                   
							
                  			
                  		<cell align="center">
						<button
						onClick="@command('editData',item=item,page='masteragency.zul')"
						iconSclass="s-icon s-icon-lapizplus" sclass="btn"/>
						<space></space>
						<button
						onClick="@command('removeData',item=item)"
						iconSclass="s-icon s-icon-trash"
						sclass="btn" />
					    </cell>
					   </row>
                  		</template>
							<foot>
						     <footer span="4" align="right" label="@load(vm.labelTotal)" />
						    </foot>
						</grid>
 </div>						
</zk>						
