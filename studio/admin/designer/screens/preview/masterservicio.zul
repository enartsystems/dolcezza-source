	   </hbox>
	   <textbox  tabindex="4" id="SERVICIOEXT" placeholder="@load(vm.lang.getProperty('SERVICIOEXT.label', 'Servicio Externo'))" value="@bind(vm.beanData.servicioext) @save(vm.beanData.servicioext, before=('grabar')) "  sclass="form-control  form-control border" visible="@load(vm.isVisible('SERVICIOEXT'))" disabled="@load(vm.isEditable('SERVICIOEXT')?false:true)"/>
       <label sclass="text-danger" value="@load(vmsgs['SERVICIOEXT'])"/>
    </h:div>   




   <h:div class="col-md-8">
     <hbox spacing="3px">
	    <label sclass="form-label font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('DESCRIPCION.label', 'Descripcion'))"/>
	   </hbox>
	   <textbox rows="4" cols="80" id="367529" multiline="true" placeholder="@load(vm.lang.getProperty('DESCRIPCION.label', 'Descripcion'))" value="@bind(vm.beanData.descripcion) @save(vm.beanData.descripcion, before=('grabar')) "  sclass="form-control  form-control border" visible="@load(vm.isVisible('DESCRIPCION'))" disabled="@load(vm.isEditable('DESCRIPCION')?false:true)"/>
       <label sclass="text-danger" value="@load(vmsgs['367529'])"/>  
</h:div>



</h:div>
</h:div>  



<h:div style="overflow:auto;" class=" col-lg-12 col-xlg-12 col-md-12">
<h:div class="row">
</h:div>
</h:div>  


             </div>      
            </h:div>
            
           </h:div>
