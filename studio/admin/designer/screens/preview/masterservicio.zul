	   <textbox  tabindex="3" id="CLASEJAVA" placeholder="@load(vm.lang.getProperty('CLASEJAVA.label', 'Clase Java'))" value="@bind(vm.beanData.clasejava) @save(vm.beanData.clasejava, before=('grabar')) "  sclass="form-control  form-control border" visible="@load(vm.isVisible('CLASEJAVA'))" disabled="@load(vm.isEditable('CLASEJAVA')?false:true)"/>
