	   </hbox>
	   <textbox  tabindex="4" id="SERVICIOEXT" placeholder="@load(vm.lang.getProperty('SERVICIOEXT.label', 'Servicio Externo'))" value="@bind(vm.beanData.servicioext) @save(vm.beanData.servicioext, before=('grabar')) "  sclass="form-control  form-control border" visible="@load(vm.isVisible('SERVICIOEXT'))" disabled="@load(vm.isEditable('SERVICIOEXT')?false:true)"/>
       <label sclass="text-danger" value="@load(vmsgs['SERVICIOEXT'])"/>
    </h:div>   




   <h:div class="col-md-8">
     <hbox spacing="3px">
	    <label sclass="form-label font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('DESCRIPCION.label', 'Descripcion'))"/>
