<zk xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul/zul.xsd"
	xmlns:h="xhtml"
	xmlns:d="http://www.zkoss.org/2005/zk/client/attribute">
		
						 
<div class="table">					 
					
				 <grid  width="100%" height="500px"	model="@load(vm.beans)" sclass="table align-middle mb-0" >  
							<columns sizable="true" menupopup="auto">
							       <column style="font-size:14px;" sclass=" text-capitalize " hflex="1" label="@load(vm.lang.getProperty('ALIAS.label', 'Alias'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['ALIASAsc'])" sortDescending="@load(vm.comparators['ALIASDesc'])"/>
							       <column style="font-size:14px;" sclass=" text-capitalize " hflex="1" label="@load(vm.lang.getProperty('ACCESKEY.label', 'Access key'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['ACCESKEYAsc'])" sortDescending="@load(vm.comparators['ACCESKEYDesc'])"/>
							       <column style="font-size:14px;" sclass=" text-capitalize " hflex="1" label="@load(vm.lang.getProperty('ACTIVE.label', 'Activo'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['ACTIVEAsc'])" sortDescending="@load(vm.comparators['ACTIVEDesc'])"/>
							       <column style="font-size:14px;" sclass=" text-capitalize " hflex="1" label="@load(vm.lang.getProperty('ALTA.label', 'Alta'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['ALTAAsc'])" sortDescending="@load(vm.comparators['ALTADesc'])"/>
							       <column style="font-size:14px;" sclass=" text-capitalize " hflex="1" label="@load(vm.lang.getProperty('EXPIRED.label', 'Fecha ExpiraciÃ³n'))" align="center" sort="auto"  sortAscending="@load(vm.comparators['EXPIREDAsc'])" sortDescending="@load(vm.comparators['EXPIREDDesc'])"/>
 							      <column style="font-size:14px;" hflex="1" label="Acciones" align="center" />
							</columns >
							<template name="model" var="item">
						       <row style="text-align:right;cursor:pointer;"  onClick="@command('editData',item=item,page='masterapykey.zul')">
							          <label  value="@load(item.alias) @converter(vm.fieldConverter)"></label>
                                   
							          <label  value="@load(item.acceskey) @converter(vm.fieldConverter)"></label>
                                   
							          <label  value="@load(item.active) @converter(vm.fieldConverter)"></label>
                                   
							          <label  value="@load(item.alta) @converter(vm.fieldConverter)"></label>
                                   
							          <label  value="@load(item.expired) @converter(vm.fieldConverter)"></label>
                                   
							
                  			
                  		<cell align="center">
						  
						<button
						onClick="@command('editData',item=item,page='masterapykey.zul')"
						iconSclass="s-icon s-icon-lapizplus" sclass="btn"    />
						<space></space>
						<button
						onClick="@command('removeData',item=item)"
						iconSclass="s-icon s-icon-trash"
						sclass="btn"   />
						
					    </cell>
					   </row>
                  		</template>
							<foot>
						     <footer span="4" align="right" label="@load(vm.labelTotal)" />
						    </foot>
						</grid>
 </div>						
</zk>						
