	   </hbox>
	   <textbox  tabindex="3" id="IDENTIFICADOR" placeholder="@load(vm.lang.getProperty('IDENTIFICADOR.label', 'DNI / NIF / NIE'))" value="@bind(vm.beanData.identificador) @save(vm.beanData.identificador, before=('grabar')) "  sclass="form-control  form-control form-control-sm border border-primary    border-danger" visible="@load(vm.isVisible('IDENTIFICADOR'))" disabled="@load(vm.isEditable('IDENTIFICADOR')?false:true)"/>
       <label sclass="text-danger" value="@load(vmsgs['IDENTIFICADOR'])"/>
    </h:div>   








    <h:div class="col-md-4">
       <hbox spacing="3px" visible="@load(vm.isVisible('FECPREVISTABAJA'))">
	    <label id="lbFECPREVISTABAJA" sclass="form-label text-capitalize  font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('FECPREVISTABAJA.label', 'Fecha Prevista de baja'))"/>
	    <a iconSclass="su-icon su-icon-question" popup="pop_FECPREVISTABAJA" />
	   </hbox>
    <datebox id="FECPREVISTABAJA"  placeholder="@load(vm.lang.getProperty('FECPREVISTABAJA.label', 'Fecha Prevista de baja'))" tabindex="4" value="@bind(vm.beanData.fecprevistabaja) @save(vm.beanData.fecprevistabaja, before=('grabar')) "   format="dd/MM/yyyy"  visible="@load(vm.isVisible('FECPREVISTABAJA'))" disabled="@load(vm.isEditable('FECPREVISTABAJA')?false:true)"/>
    <label sclass="text-danger" value="@load(vmsgs['FECPREVISTABAJA'])"/>
