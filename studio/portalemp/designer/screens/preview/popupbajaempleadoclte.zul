<combobox mold="rounded" autodrop="true"  id="idrrhhmotivobaja" placeholder="@load(vm.lang.getProperty('idrrhhmotivobaja').label,'Motivo de Baja')" model="@bind(vm.foreings['idrrhhmotivobaja'])"  buttonVisible="true"	onSelect="@command('onSelectForeing', alias='idrrhhmotivobaja',item=self.selectedItem.value)"  		selectedItem="@load(vm.beanData.idrrhhmotivobaja)"  onCreate="@command('onCreateCombo',component=self,alias='idrrhhmotivobaja')" value="@bind(vm.beanData.idrrhhmotivobaja .motivobaja)@save(vm.beanData.idrrhhmotivobaja.motivobaja, before=('grabar')) @validator(vm.validator,required=true)"  ><template name="model" var="item"><comboitem value="@load(item)" label="@load(item.motivobaja)" /></template></combobox>
 <label sclass="text-danger" value="@load(vmsgs['idrrhhmotivobaja'])"/>
   <popup id="pop_oICJiflEaE" width="200px">
	  <vbox>  
     <fragment exps="@load(vm.lang.getProperty('idrrhhmotivobaja.helptext', 'idrrhhmotivobaja'))" >
	 
	    	<![CDATA[
<span textContent='@load(exps)'></span>
	    	  	]]>        
	  </fragment></vbox>
  </popup>
</h:div>


   <h:div class="col-md-12">
     <hbox spacing="3px">
	    <label sclass="form-label font-weight-normal align-text-middle" style="" value="@load(vm.lang.getProperty('MOTIVOBAJA.label', 'Ninguno'))"/>
	   </hbox>
	   <textbox rows="4" cols="80" id="565531" multiline="true" placeholder="@load(vm.lang.getProperty('MOTIVOBAJA.label', 'Ninguno'))" value="@bind(vm.beanData.motivobaja) @save(vm.beanData.motivobaja, before=('grabar')) "  sclass="form-control  form-control border" visible="@load(vm.isVisible('MOTIVOBAJA'))" disabled="@load(vm.isEditable('MOTIVOBAJA')?false:true)"/>
       <label sclass="text-danger" value="@load(vmsgs['565531'])"/>  
</h:div>



         </h:div> 
       </h:div>
  </h:div>
 </h:div>  
          
                              
          
        
<h:div style="overflow:auto;" class=" col-lg-12 col-xlg-12 col-md-12">
<h:div class="row">
</h:div>
</h:div>                     </div>           
            </h:div>
            <div class="hstack gap-2 justify-content-end">
								
									<button onClick="@command('store')"
										class="btn btn-success" >
										Guardar
									</button>
									<button onClick="@command('storeNew')"
										class="btn " >
										Guardar y Nuevo
									</button>
									<button onClick="@command('cerrarPopup')"
										class="btn btn-light" >
										Cerrar
									</button>
								
							</div>
           </h:div>
          </h:div>
         </div>   
        		</panelchildren>
		</panel> 
            <fragment exps="@load(vm.lang['bajaempleadoclte.screen'])" >
				<![CDATA[
				<div id="modal_iconified" class="modal fade" tabindex="-1" style="z-index: 99999;">
					<div class="modal-dialog">
						<div class="modal-content">
							<div class="modal-header">
								<h5 class="modal-title"><span textContent='@load(exps)'></span></h5>
							</div>
							<div class="modal-body">
								<h6 class="font-weight-semibold"><i class="icon-law mr-2"></i>Texto de ayuda pantalla</h6>
								<span textContent='@load(exps)'></span>
							</div>
							<div class="modal-footer">
								<button class="btn btn-link" data-dismiss="modal"><i class="icon-cross2 font-size-base mr-1"></i> Close</button>
							</div>
						</div>
					</div>
				</div>
