	   <textbox  tabindex="2" id="APELLIDOS" placeholder="@load(vm.lang.getProperty('APELLIDOS.label', 'Apellidos'))" value="@bind(vm.beanData.apellidos) @save(vm.beanData.apellidos, before=('grabar')) "  sclass="form-control  form-control form-control-sm border border-primary bg-light text-dark border-danger" visible="@load(vm.isVisible('APELLIDOS'))" disabled="@load(vm.isEditable('APELLIDOS')?false:true)"/>
